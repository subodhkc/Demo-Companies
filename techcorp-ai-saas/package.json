{
  "name": "techcorp-ai-platform",
  "version": "2.8.0",
  "description": "Enterprise AI-powered analytics platform",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc && webpack --mode production",
    "test": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "security:scan": "npm audit && snyk test",
    "security:fix": "npm audit fix && snyk fix",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "docker:build": "docker build -t techcorp-ai:latest .",
    "docker:scan": "trivy image techcorp-ai:latest",
    "deploy:staging": "npm run build && npm run deploy -- --env staging",
    "deploy:production": "npm run build && npm run deploy -- --env production"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-secrets-manager": "^3.450.0",
    "@aws-sdk/client-kms": "^3.450.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "openai": "^4.20.1",
    "@tensorflow/tfjs-node": "^4.15.0",
    "bull": "^4.12.0",
    "socket.io": "^4.6.0",
    "prom-client": "^15.1.0",
    "dd-trace": "^4.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^2.1.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "@playwright/test": "^1.40.1",
    "snyk": "^1.1275.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/techcorp-ai/platform.git"
  },
  "author": "TechCorp AI Engineering Team",
  "license": "PROPRIETARY",
  "private": true
}
