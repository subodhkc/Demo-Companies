# Application
NODE_ENV=production
PORT=8080
APP_NAME=TechCorp AI Platform
APP_VERSION=2.8.0

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/techcorp_prod
DATABASE_POOL_MIN=10
DATABASE_POOL_MAX=50
DATABASE_SSL=true

# Redis
REDIS_URL=redis://localhost:6379
REDIS_TLS=true
REDIS_PASSWORD=

# AWS
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=techcorp-customer-data
AWS_KMS_KEY_ID=

# Secrets Manager
SECRETS_MANAGER_ENABLED=true
DATABASE_SECRET_ARN=arn:aws:secretsmanager:us-east-1:123456789012:secret:prod/database
API_KEYS_SECRET_ARN=arn:aws:secretsmanager:us-east-1:123456789012:secret:prod/api-keys

# Security
JWT_SECRET=
JWT_EXPIRY=24h
ENCRYPTION_KEY=
SESSION_SECRET=
MFA_ISSUER=TechCorp AI

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
CORS_ORIGIN=https://app.techcorp-ai.com
CORS_CREDENTIALS=true

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
DATADOG_API_KEY=
DATADOG_APP_KEY=

# Monitoring
SENTRY_DSN=
SENTRY_ENVIRONMENT=production
SENTRY_TRACES_SAMPLE_RATE=0.1

# OpenAI
OPENAI_API_KEY=
OPENAI_ORG_ID=
OPENAI_MODEL=gpt-4-turbo-preview

# Email
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=noreply@techcorp-ai.com

# Feature Flags
ENABLE_MFA=true
ENABLE_AI_FEATURES=true
ENABLE_ANALYTICS=true
ENABLE_AUDIT_LOGGING=true

# Compliance
SOC2_MODE=enabled
GDPR_MODE=enabled
DATA_RESIDENCY_ENFORCEMENT=true
AUDIT_LOG_RETENTION_DAYS=365

# Performance
CACHE_TTL=3600
MAX_UPLOAD_SIZE=10485760
REQUEST_TIMEOUT=30000

# Multi-tenancy
TENANT_ISOLATION=strict
TENANT_ENCRYPTION=enabled
